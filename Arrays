for (const [clave, valor] of Object.entries(excepciones)) {
    const aExc = document.createElement('a');
    aExc.classList.add('list-group-item');
    aExc.classList.add('list-group-item-action');
    aExc.setAttribute('href', "#");
    aExc.setAttribute('aria-current', "true");

    const divExc = document.createElement('div');
    divExc.classList.add('d-flex');
    divExc.classList.add('w-100');
    divExc.classList.add('justify-content-between');

    const divExcH5 = document.createElement('h5');
    divExcH5.classList.add('mb-1');
    divExcH5.textContent = clave; // Agregar el texto correspondiente

    const pExc = document.createElement('p');
    pExc.classList.add('mb-1');
    pExc.textContent = valor; // Agregar el texto correspondiente

    // Agregar los elementos al Ã¡rbol DOM
    divExc.appendChild(divExcH5);
    aExc.appendChild(divExc);
    aExc.appendChild(pExc);
    listData.appendChild(aExc);
}
