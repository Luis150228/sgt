document.getElementById("btn-select-all").addEventListener("click", () => {
    // Obtén todas las casillas de verificación y conviértelas a un array
    const checkboxes = Array.from(document.querySelectorAll('.form-check-input[data-check="true"]'));

    // Filtra las casillas de verificación marcadas y obtén sus valores
    const valoresMarcados = checkboxes.filter(checkbox => checkbox.checked).map(checkbox => checkbox.value);

    // Imprime en la consola los valores de las casillas marcadas
    console.log("Casillas marcadas:", valoresMarcados);
});


<script>
    document.getElementById("btn-select-all").addEventListener("click", function() {
        // Obtén todas las casillas de verificación
        var checkboxes = document.querySelectorAll('.form-check-input[data-check="true"]');
    
        // Filtra las casillas de verificación marcadas
        var casillasMarcadas = Array.from(checkboxes).filter(function(checkbox) {
            return checkbox.checked;
        });
    
        // Obtén los valores de las casillas marcadas
        var valoresMarcados = casillasMarcadas.map(function(checkbox) {
            return checkbox.value;
        });
    
        // Imprime en la consola los valores de las casillas marcadas
        console.log("Casillas marcadas:", valoresMarcados);
    });
    </script>