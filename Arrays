google.charts.load('current', {packages:['treemap']});
const totalStockActivo = () => {
	const data = new google.visualization.DataTable();
	data.addColumn('string', 'ID');
	data.addColumn('string', 'Parent');
	data.addColumn('number', 'Stock');
	data.addRows(totalStock);

	const tree = new google.visualization.TreeMap(document.getElementById("barchart_equipos"));

	const options = {
		enableHighlight: true,
		maxDepth: 1,
		maxPostDepth: 2,
		minHighlightColor: '#fa3939',
		midHighlightColor: '#753f3f',
		maxHighlightColor: '#575757',
		minColor: '#575757',
		midColor: '#995757',
		maxColor: '#fa3939',
		headerHeight: 15,
		showScale: true,
		generateTooltip: showFullTooltip,
		height: 300,
		useWeightedAverageForAggregation: true,
		titleTextStyle: {
			font: 16,
			color: '#ffffff',
		},
		legend: {
			textStyle: {
				color: '#ffffff',
			},
		},
		// Use click to highlight and double-click to drill down.
		eventsConfig: {
			highlight: ['mousein'],
			unhighlight: ['mouseout'],
			rollup: ['dblclick'],
			drilldown: ['click'],
		},
	};

	tree.draw(data, options);
	function showFullTooltip(row, size, value) {
		const rowData = data.getValue(row, 0);
		const TotalValue = data.getValue(row, 2);
		const nuevoValue = data.getValue(row, 3);
		const abiertoValue = data.getValue(row, 4);
		const cursoValue = data.getValue(row, 5);
		const pendienteValue = data.getValue(row, 6);

		return `
      <div style="background:#272B30; padding:10px; border-style:none">
        <span style="font-family:Verdana"><b>${rowData}</b>,</span><br>
        Total: ${TotalValue}<br>
        ${data.getColumnLabel(3)}: ${nuevoValue}<br>
        ${data.getColumnLabel(4)}: ${abiertoValue}<br>
        ${data.getColumnLabel(5)}: ${cursoValue}<br>
        ${data.getColumnLabel(6)}: ${pendienteValue}<br>
      </div>
    `;
	}
};
google.charts.setOnLoadCallback(totalStockActivo);