quiero saber como funcionan las siguientes clases en JS de los siguietes dor archivos y como los implemento en mi codigo

almacen.class.js: 
const regDuble = /"/gi;
const regSingel = /'/gi;

export class Odc {
    constructor(factura, proveedor, equipo, cantidad, almacen, motivo, observacion, adjunto, responsable){
        this.id = new Date().getTime();
        this.num_pedido = factura;
        this.npvr = proveedor;
        this.idequipo = equipo;
        this.cantidad = cantidad;
        this.entrada = new Date().getTime();
        this.almacen = almacen;
        this.motivo = motivo;
        this.observaciones = observacion;
        this.adjunto = adjunto;
        this.responsable = responsable
    }

}

export class OdcAdjunto {
    constructor(factura, adjunto){
        this.id = new Date().getTime();
        this.num_pedido = factura;
        this.adjunto = adjunto;
    }

}

y almacen-list.class.js:

export class OdcManager{
    constructor(){
        this.elementos = []
    }
    agregarElemento(elemento){
        this.elementos.push(elemento)
    }
    eliminarElemento(){
        this.elementos = this.elementos
    }
    modificarElemento(id, factura, proveedor, equipo, cantidad, almacen, motivo, observacion, adjunto, responsable) {
        for (const elemento of this.elementos) {
            if (elemento.id == id) {
                elemento.num_pedido = factura;
                elemento.npvr = proveedor;
                elemento.idequipo = equipo;
                elemento.cantidad = cantidad;
                elemento.almacen = almacen;
                elemento.motivo = motivo;
                elemento.observaciones = observacion;
                elemento.adjunto = adjunto;
                elemento.responsable = responsable
                break;
            }
        }
    }
}
