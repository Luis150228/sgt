tengo la siguiente consulta en un store produre en mysql marida DB y quiero una modificacion en la que si el registro que va insertar ya esta en la tabla de destino solo actualice los datos, la clave de la tabla destino es ticket se puede hacer un ON DUPLICATE KEY UPDATE?

INSERT INTO eut_weeklyreportsinc SELECT folio AS 'Ticket', folio as 'id Externo', abierto as 'Fecha de Envio', resuelto as 'Fecha resolucion', Tiempo, categoria as 'Nivel 1', subcategoria as 'Nivel 2', nombreproyecto as 'Nivel 3', grup_global as 'Grupo Asignado', asig_a as 'Usuario Asignado', ubicacion as 'Localidad', zona_num as 'Zona', obs_notasresolucion as 'Observaciones', cerrado as 'Fecha de Cierre', estatus as 'Estatus', slak AS 'Inhibidor', ubicacion AS 'Ubicacion', area AS 'Localizacion', '' AS 'Codigo', '' AS 'Descripcion', '' AS 'INHIBIDOR_CODIGO', '' AS 'CodigoDescripcion', '' AS 'Descripcion_Resolutor', '' AS 'Comentarios_Resolutor', '' AS 'Regional', '' AS 'Procede', 'En Metrica' AS 'CCG', let_rango AS 'SEMANAL', var_reporte FROM (SELECT folio, abierto, resuelto, cerrado, e.edo_eut, categoria, subcategoria, tipoproblema, nombreproyecto, grup_asig, asig_a, grup_global, grup_tipo, ubicacion, ifnull(suc_corp, 'Corporativo') as 'area', grup_tareas, IF(LEFT(folio,3)='INC', 'Incidente', 'Tareas') as 'tipo', s.ceco, r.region as 'region_cau', ifnull(r.zona_cau, g.grup_zona) as 'zona_num', fn_zonasla(grup_global, ifnull(r.zona_cau, g.grup_zona)) AS 'SLA', fn_eut_min(abierto, resuelto, s.grup_asig) AS 'Tiempo', if(fn_eut_min(abierto, resuelto, s.grup_asig)>fn_zonasla(grup_global, ifnull(r.zona_cau, g.grup_zona)), 'VENCIDO', 'EN TIEMPO') AS 'estatus', limitadores, obs_notasresolucion, concat(LEFT(if(fn_eut_min(abierto, resuelto, s.grup_asig)>fn_zonasla(grup_global, ifnull(r.zona_cau, g.grup_zona)), 'VENCIDO', 'EN TIEMPO'), 1), LEFT(limitadores,1)) as 'slak', incidencia_principal as 'num_relacionado' FROM servicenow_reportes s 
LEFT JOIN eut_estatus e ON s.estatus = e.estatus
LEFT JOIN eut_grupos g ON s.grup_asig = g.grup_asignado
LEFT JOIN eut_redsuc r ON s.ceco = r.ceco)r
		WHERE folio LIKE 'INC%' AND grup_asig like '%cau_zon%' AND edo_eut != 'Cancelado' AND (resuelto between @diaInicio AND @diaFin) AND slak != 'VK' AND num_relacionado NOT LIKE 'INC%' ORDER BY zona_num ASC, estatus DESC, tiempo DESC;
        