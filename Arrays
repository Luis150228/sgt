// Tu arreglo anidado
const data = [
    [...],  // Primer grupo de elementos
    [...],  // Segundo grupo de elementos
    [...],  // Tercer grupo de elementos
];

// Función para buscar elementos que cumplan con ciertos criterios
function buscarElementos(criterio, valor) {
    const resultados = [];

    for (const grupo of data) {
        for (const elemento of grupo) {
            if (criterio in elemento && elemento[criterio] === valor) {
                resultados.push(elemento);
            }
        }
    }

    return resultados;
}

// Ejemplo de uso
const criterioBusqueda = "motivo";
const valorBusqueda = "Renovacion de baterias";

const resultados = buscarElementos(criterioBusqueda, valorBusqueda);

if (resultados.length > 0) {
    for (const resultado of resultados) {
        console.log(resultado);
    }
} else {
    console.log("No se encontraron elementos que cumplan con los criterios de búsqueda.");
}
