"<?php echo date('Y-m-d', strtotime('-1 day')); ?>"

// Cargar la biblioteca de Google Charts
google.charts.load('current', {'packages':['corechart']});
google.charts.setOnLoadCallback(drawChart);

// Función para dibujar el gráfico de barras
function drawChart() {
    // Tu conjunto de datos
    const dataObject = {
        "27 OCT - 02 NOV CAU_INC": {
            "atendidos": 422,
            // ... (otros datos)
        },
        // ... (otros conjuntos de datos)
    };

    // Convertir el objeto en un array de arrays
    const dataArray = Object.entries(dataObject).map(([key, value]) => [key, value.atendidos, value.enTiempo, value.excepciones]);

    // Agregar los encabezados al principio del array
    dataArray.unshift(['Semana', 'Atendidos', 'En Tiempo', 'Excepciones']);

    // Crear un objeto de datos de Google Charts
    const data = google.visualization.arrayToDataTable(dataArray);

    // Opciones del gráfico
    const options = {
        title: 'Ejemplo de Gráfico de Barras',
        chartArea: {width: '50%'},
        hAxis: {
            title: 'Cantidad',
            minValue: 0
        },
        vAxis: {
            title: 'Semana'
        }
    };

    // Crear el gráfico de barras
    const chart = new google.visualization.BarChart(document.getElementById('chart_div'));

    // Dibujar el gráfico
    chart.draw(data, options);
}

<div id="chart_div" style="width: 800px; height: 400px;"></div>
