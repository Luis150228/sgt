function agruparPorMarca(equipos) {
  const agrupados = {};

  for (const equipo of equipos) {
    const marca = equipo.marca;

    if (agrupados[marca]) {
      agrupados[marca].push({ marca, npvr: equipo.npvr });
    } else {
      agrupados[marca] = [{ marca, npvr: equipo.npvr }];
    }
  }

  return agrupados;
}


