$incidentesCAU = array_filter($data, function($item) {
    return $item['type'] === 'Incidentes CAU';
});

$incidentesSDK = array_filter($data, function($item) {
    return $item['type'] === 'Incidentes SDK';
});

$solicitudesCAU = array_filter($data, function($item) {
    return $item['type'] === 'Solicitudes CAU';
});

$solicitudesSDK = array_filter($data, function($item) {
    return $item['type'] === 'Solicitudes SDK';
});

$totalIncidentes = count($incidentesCAU) + count($incidentesSDK);
$totalSolicitudes = count($solicitudesCAU) + count($solicitudesSDK);

$porcentajeIncidentesCAU = (count($incidentesCAU) / $totalIncidentes) * 100;
$porcentajeIncidentesSDK = (count($incidentesSDK) / $totalIncidentes) * 100;

$porcentajeSolicitudesCAU = (count($solicitudesCAU) / $totalSolicitudes) * 100;
$porcentajeSolicitudesSDK = (count($solicitudesSDK) / $totalSolicitudes) * 100;

echo "Incidentes CAU: " . count($incidentesCAU) . " (" . round($porcentajeIncidentesCAU, 2) . "%)\n";
echo "Incidentes SDK: " . count($incidentesSDK) . " (" . round($porcentajeIncidentesSDK, 2) . "%)\n";
echo "Total de Incidentes: " . $totalIncidentes . "\n\n";

echo "Solicitudes CAU: " . count($solicitudesCAU) . " (" . round($porcentajeSolicitudesCAU, 2) . "%)\n";
echo "Solicitudes SDK: " . count($solicitudesSDK) . " (" . round($porcentajeSolicitudesSDK, 2) . "%)\n";
echo "Total de Solicitudes: " . $totalSolicitudes . "\n";
