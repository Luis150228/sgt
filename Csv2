usando tus indicacioens manda error

<ul id="ul-pag" class="pagination justify-content-end"></ul>

const ulBtns = document.getElementById('ul-pag')

ulBtns.addEventListener('click', e =>{
  console.log(e.target.dataset.i);
  const zoneBtns = e.target
  if (zoneBtns === this.firstElementChild) {
      console.log('Primero');
  }
  if (zoneBtns === this.lastElementChild) {
      console.log('Ultimo');
  }
})

